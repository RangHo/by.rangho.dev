{
    "rewrites": [
        {
            "source": "/vercel-hello-world/:path*",
            "destination": "https://rangho-vercel-hello-world.vercel.app/:path*",
            "permanent": false
        },
        {
            "source": "*",
            "destination": "https://rangho.dev/",
            "permanent": false,
            "statusCode": 301
        }
    ]
}
